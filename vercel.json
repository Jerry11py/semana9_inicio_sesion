{
  "builds": [
    {
      "src": "loginApp/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",  // Ruta para servir archivos estáticos
      "dest": "/static/$1"    // Servir directamente desde el directorio de estáticos
    },
    {
      "src": "/(.*)",
      "dest": "loginApp/wsgi.py"
    }
  ]
}
